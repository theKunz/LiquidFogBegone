{
    "schemaVersion": 1,
    "id": "liquidfogbegone",
    "version": "${version}",
    "name": "Liquid Fog Begone",
    "description": "A simple mod that allows for the control of the opacity of fog while in water or lava",
    "authors": [
        "theKunz"
    ],
    "contact": {
        "sources": "https://github.com/theKunz/LiquidFogBegone"
    },
    "license": "MIT",
    "icon": "assets/liquidfogbegone/icon.png",
    "environment": "client",
    "entrypoints": {
        "main": [
           "com.liquidfogbegone.LiquidFogBegone"
        ],
        "client": [
            "com.liquidfogbegone.LiquidFogBegoneClient"
        ],
        "modmenu": [
            "com.liquidfogbegone.config.ModMenuApiImpl"
        ]
    },
    "mixins": [
        "liquidfogbegone.mixins.json",
        {
            "config": "liquidfogbegone.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=0.16.9",
        "minecraft": "~1.21.7",
        "java": ">=21",
        "fabric-api": "*"
    }
}
